pipeline {
    agent any

    stages {
        stage('Sumar') {
            steps {
                script {
                    sh 'python3 hola.py'
                }
            }
        }
        stage('Restar') {
            steps {
                script {
                    sh 'python3 hola.py'
                }
            }
        }
        stage('Multiplicar') {
            steps {
                script {
                    sh 'python3 hola.py'
                }
            }
        }
        stage('Dividir') {
            steps {
                script {
                    sh 'python3 hola.py'
                }
            }
        }
    }
}
